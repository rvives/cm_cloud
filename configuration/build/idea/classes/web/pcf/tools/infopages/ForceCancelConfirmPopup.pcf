<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../../pcf.xsd">
  <Popup
    canEdit="true"
    id="ForceCancelConfirmPopup"
    startInEditMode="true"
    title="DisplayKey.get(&quot;Web.InternalTools.InfoPages.UpgradeInfo.ForceCancel.Title&quot;)">
    <LocationEntryPoint
      signature="ForceCancelConfirmPopup(Page : gw.api.tools.UpgradeInfoPageHelper)"/>
    <Variable
      name="Page"
      type="gw.api.tools.UpgradeInfoPageHelper"/>
    <Variable
      initialValue="false"
      name="checked"
      type="java.lang.Boolean"/>
    <Variable
      initialValue="Page.ServerIdDoingRollingUpgrade"
      name="serverId"
      type="java.lang.String"/>
    <Screen
      id="ForceCancelConfirmScreen">
      <DetailViewPanel>
        <InputColumn
          labelWidth="900">
          <TextInput
            id="Warning"
            label="DisplayKey.get(&quot;Web.InternalTools.InfoPages.UpgradeInfo.ForceCancel.Warning&quot;)"
            value="&quot;&quot;"/>
          <TextInput
            id="Confirm"
            label="DisplayKey.get(&quot;Web.InternalTools.InfoPages.UpgradeInfo.ForceCancel.Confirm&quot;)"
            value="&quot;&quot;"/>
        </InputColumn>
      </DetailViewPanel>
      <PanelRef
        id="toolbar">
        <Toolbar>
          <ToolbarButton
            action="CurrentLocation.commit(); Page.forceCancelIncrementalUpgrade()"
            available="checked"
            id="Continue"
            label="DisplayKey.get(&quot;Web.InternalTools.InfoPages.UpgradeInfo.ForceCancel.Continue&quot;)"/>
          <ToolbarButton
            action="CurrentLocation.cancel()"
            id="Cancel"
            label="DisplayKey.get(&quot;Button.Cancel&quot;)"/>
        </Toolbar>
        <ListViewPanel>
          <Row>
            <CheckBoxCell
              editable="true"
              id="Checked"
              value="checked">
              <PostOnChange/>
            </CheckBoxCell>
            <TextCell
              id="ServerId"
              label="DisplayKey.get(&quot;Web.InternalTools.InfoPages.UpgradeInfo.ForceCancel.ServerId&quot;)"
              value="serverId"/>
          </Row>
        </ListViewPanel>
      </PanelRef>
    </Screen>
  </Popup>
</PCF>
