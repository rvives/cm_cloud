<?xml version="1.0" encoding="UTF-8"?>
<!-- US -->
<ZonesInputSet xmlns="http://guidewire.com/zones-input-set">
  <ZonesInput screenType="BusinessWeek">
    <ZoneTypes>
      <ZoneType code="city" selectionType="shuttleList" />
      <ZoneType code="country" selectionType="shuttleList"/>
      <ZoneType code="county" selectionType="shuttleList" filterBy="state"/>
      <ZoneType code="state" selectionType="shuttleList"/>
      <ZoneType code="zip" selectionType="listView"/>
    </ZoneTypes>
  </ZonesInput>

  <ZonesInput screenType="Holiday">
    <ZoneTypes>
      <ZoneType code="city" selectionType="shuttleList" />
      <ZoneType code="country" selectionType="shuttleList"/>
      <ZoneType code="county" selectionType="shuttleList" filterBy="state"/>
      <ZoneType code="state" selectionType="shuttleList"/>
      <ZoneType code="zip" selectionType="listView"/>
    </ZoneTypes>
  </ZonesInput>

  <ZonesInput screenType="Region">
    <ZoneTypes>
      <ZoneType code="county" selectionType="shuttleList" filterBy="state"/>
      <ZoneType code="state" selectionType="shuttleList"/>
      <ZoneType code="zip" selectionType="listView"/>
    </ZoneTypes>
  </ZonesInput>

</ZonesInputSet>