{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {
    "BranchCopied": {
      "type": "object",
      "properties": {
        "source": {
          "description": "Id of source branch",
          "type": "string"
        },
        "target": {
          "description": "Id of target branch",
          "type": "string"
        },
        "start": {
          "description": "New period start date/time",
          "type": "string",
          "format": "date-time"
        },
        "end": {
          "description": "New period end date/time",
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "PeriodChanged": {
      "type": "object",
      "properties": {
        "branch": {
          "description": "Id of branch",
          "type": "string"
        },
        "start": {
          "description": "New period start date/time",
          "type": "string",
          "format": "date-time"
        },
        "end": {
          "description": "New period end date/time",
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "SliceCopied": {
      "type": "object",
      "properties": {
        "source": {
          "description": "Id of source branch",
          "type": "string"
        },
        "target": {
          "description": "Id of target branch",
          "type": "string"
        },
        "date": {
          "description": "Date of slice to copy",
          "type": "string",
          "format": "date-time"
        }
      }
    }
  }
}
